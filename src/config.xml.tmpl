<?xml version='1.0' encoding='utf-8'?>
<widget id="com.imetrical.dev.pgngOne" version="1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name><%= appName %></name>
    <description>
        Angular and phonegap living in a tree.
    </description>
    <author email="your@email.com" href="http://example.com">
        Daniel lauzon
    </author>
    <access origin="*" />

    <!-- Override PhoneGap default configurations here -->
    <preference name="UIWebViewBounce" value="false" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
